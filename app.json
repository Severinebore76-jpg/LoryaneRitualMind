{
  "expo": {
    "name": "Orelys RitualMind",
    "slug": "Orelys-Ritual-Mind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icone_orelys.png",
    "scheme": "orelysritualmind",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orelys.ritualmind"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#dccfcf",
        "foregroundImage": "./assets/images/icone_orelys.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/icone_orelys.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_orelys.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#dccfcf",
          "dark": {
            "backgroundColor": "#241718"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
